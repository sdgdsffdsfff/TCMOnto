###  http://ccnt.zju.edu.cn/drugnet/drug#broaderTransitive

:broaderTransitive rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "传递性的广义概念"@zh .



###  http://ccnt.zju.edu.cn/drugnet/drug#contains

:contains rdf:type owl:ObjectProperty ;
          
          rdfs:label "包含"@zh .



###  http://ccnt.zju.edu.cn/drugnet/drug#containsChemical

:containsChemical rdf:type owl:ObjectProperty ;
                  
                  rdfs:label "包含化学物质"@zh ;
                  
                  rdfs:range :Chemical ;
                  
                  rdfs:domain :Herb ;
                  
                  rdfs:subPropertyOf :contains .



###  http://ccnt.zju.edu.cn/drugnet/drug#containsHerb

:containsHerb rdf:type owl:ObjectProperty ;
              
              rdfs:label "包含草药"@zh ;
              
              rdfs:domain :Formula ;
              
              rdfs:range :Herb ;
              
              rdfs:subPropertyOf :contains .



###  http://ccnt.zju.edu.cn/drugnet/drug#hasHerbalTaste

:hasHerbalTaste rdf:type owl:ObjectProperty ;
                
                rdfs:label "具有药味"@zh ;
                
                rdfs:domain :Herb ;
                
                rdfs:range :HerbalTaste .



###  http://ccnt.zju.edu.cn/drugnet/drug#hasKing

:hasKing rdf:type owl:ObjectProperty ;
         
         rdfs:subPropertyOf :containsHerb .



###  http://ccnt.zju.edu.cn/drugnet/drug#hasMessenger

:hasMessenger rdf:type owl:ObjectProperty ;
              
              rdfs:subPropertyOf :containsHerb .



###  http://ccnt.zju.edu.cn/drugnet/drug#hasMinister

:hasMinister rdf:type owl:ObjectProperty ;
             
             rdfs:subPropertyOf :containsHerb .



###  http://ccnt.zju.edu.cn/drugnet/drug#hasSubordinate

:hasSubordinate rdf:type owl:ObjectProperty ;
                
                rdfs:subPropertyOf :containsHerb .



###  http://ccnt.zju.edu.cn/drugnet/drug#narrowerTransitive

:narrowerTransitive rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "传递性的狭义概念"@zh .



###  http://ccnt.zju.edu.cn/drugnet/drug#prescribe

:prescribe rdf:type owl:ObjectProperty ;
           
           rdfs:label "开处方"@zh ;
           
           rdfs:range :Drug ;
           
           rdfs:domain :Therapy .



###  http://ccnt.zju.edu.cn/drugnet/drug#relatedTo

:relatedTo rdf:type owl:ObjectProperty ,
                    owl:SymmetricProperty ;
           
           rdfs:label "相关"@zh ;
           
           rdfs:domain :ClinicalCondition ;
           
           rdfs:range :ClinicalCondition .



###  http://ccnt.zju.edu.cn/drugnet/drug#treats

:treats rdf:type owl:ObjectProperty ;
        
        rdfs:label "治疗"@zh ;
        
        rdfs:range :ClinicalCondition ;
        
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Drug
                                    :Therapy
                                  )
                    ] .





